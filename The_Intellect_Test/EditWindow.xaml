        <Button x:Name="button_pictureBox1" HorizontalAlignment="Left" Margin="24,79,0,0" VerticalAlignment="Top" Width="52" Background="{x:Null}" BorderBrush="Black" Height="69" Click="button_pictureBox1_Click" Opacity="0.2"/>
        <Image x:Name="pictureBox2" Source="images/pic2.png" HorizontalAlignment="Left" Height="69" Margin="102,80,0,0" VerticalAlignment="Top" Width="52"/>
