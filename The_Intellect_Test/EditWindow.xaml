        <TextBlock x:Name="textBlock_password" HorizontalAlignment="Left" Margin="494,158,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Foreground="Red" FontSize="10.667"/>
        <TextBlock x:Name="textBlock_password2" HorizontalAlignment="Left" Margin="494,189,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Foreground="Red" FontSize="10.667"/>
        <Label Content="?" Cursor="Help" FontSize="10" Foreground="#FF0060FF" Height="28" HorizontalAlignment="Left" Margin="480,81,0,0" Name="label_question1" ToolTip="Не использовать кириллицу и спец. символы, мин. 4 сим" VerticalAlignment="Top" />
