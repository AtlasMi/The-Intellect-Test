        <Border BorderBrush="Black" BorderThickness="1" HorizontalAlignment="Left" Height="70" Margin="97,231,0,0" VerticalAlignment="Top" Width="53"/>
        <Border BorderBrush="Black" BorderThickness="1" HorizontalAlignment="Left" Height="68" Margin="22,231,0,0" VerticalAlignment="Top" Width="53"/>
        <Border BorderBrush="Black" BorderThickness="1" HorizontalAlignment="Left" Height="70" Margin="171,154,0,0" VerticalAlignment="Top" Width="52"/>
        <Border BorderBrush="Black" BorderThickness="1" HorizontalAlignment="Left" Height="68" Margin="97,156,0,0" VerticalAlignment="Top" Width="53"/>
