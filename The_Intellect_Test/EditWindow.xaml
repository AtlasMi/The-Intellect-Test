        <Label x:Name="label_password" Content="Новый пароль:" HorizontalAlignment="Left" Margin="271,151,0,0" VerticalAlignment="Top"/>
        <Button x:Name="button_ok" Content="OK" HorizontalAlignment="Left" Height="20" Margin="384,221,0,0" VerticalAlignment="Top" Width="75" Cursor="Hand" Click="button_ok_Click" />
