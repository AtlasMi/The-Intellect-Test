        <PasswordBox x:Name="passwordBox_2" HorizontalAlignment="Left" Height="23" Margin="364,186,0,0" VerticalAlignment="Top" Width="118" MaxLength="10" PasswordChanged="passwordBox_2_PasswordChanged"/>
        <Label x:Name="label_selectimage" Content="Выберите аватар:" HorizontalAlignment="Left" Margin="67,49,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label_oldpassword" Content="Старый пароль:" HorizontalAlignment="Left" Margin="268,122,0,0" VerticalAlignment="Top"/>
        <PasswordBox x:Name="oldpasswordBox" HorizontalAlignment="Left" Height="23" Margin="364,125,0,0" VerticalAlignment="Top" Width="118" MaxLength="10" PasswordChanged="passwordBox_PasswordChanged"/>
        <Button x:Name="button_reset" Content="Сброс" HorizontalAlignment="Left" Height="27" Margin="505,276,0,0" VerticalAlignment="Top" Width="88" Cursor="Hand" Click="button_reset_Click" />
