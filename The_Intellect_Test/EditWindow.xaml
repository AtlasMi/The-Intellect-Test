        <Image x:Name="pictureBox9" Source="images/pic9.png" HorizontalAlignment="Left" Height="69" Margin="171,235,0,0" VerticalAlignment="Top" Width="52"/>
        <Button x:Name="button_pictureBox9" HorizontalAlignment="Left" Margin="171,232,0,0" VerticalAlignment="Top" Width="52" Background="{x:Null}" BorderBrush="Black" Height="69" Click="button_pictureBox9_Click" Opacity="0.2"/>
