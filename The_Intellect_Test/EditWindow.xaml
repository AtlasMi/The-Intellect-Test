        <TextBlock x:Name="textBlock_pictureBox2_check" HorizontalAlignment="Left" Margin="82,108,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Foreground="#FF008000" Width="13"/>
        <TextBlock x:Name="textBlock_pictureBox3_check" HorizontalAlignment="Left" Margin="158,108,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Foreground="#FF008000" Width="13"/>
        <TextBlock x:Name="textBlock_pictureBox4_check" HorizontalAlignment="Left" Margin="6,183,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Foreground="#FF008000" Width="13"/>
        <TextBlock x:Name="textBlock_pictureBox5_check" HorizontalAlignment="Left" Margin="82,186,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Foreground="#FF008000" Width="13"/>
        <TextBlock x:Name="textBlock_pictureBox6_check" HorizontalAlignment="Left" Margin="158,186,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Foreground="#FF008000" Width="13"/>
        <TextBlock x:Name="textBlock_pictureBox7_check" HorizontalAlignment="Left" Margin="6,257,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Foreground="#FF008000" Width="13"/>
        <TextBlock x:Name="textBlock_pictureBox8_check" HorizontalAlignment="Left" Margin="82,260,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Foreground="#FF008000" Width="13"/>
        <TextBlock x:Name="textBlock_pictureBox9_check" HorizontalAlignment="Left" Margin="158,260,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Foreground="#FF008000" Width="13"/>
