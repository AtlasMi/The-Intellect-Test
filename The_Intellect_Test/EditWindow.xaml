        <Image x:Name="pictureBox6" Source="images/pic6.png" HorizontalAlignment="Left" Height="69" Margin="171,157,0,0" VerticalAlignment="Top" Width="52"/>
        <Button x:Name="button_pictureBox6"  HorizontalAlignment="Left" Margin="171,155,0,0" VerticalAlignment="Top" Width="52" Background="{x:Null}" BorderBrush="Black" Height="69" Click="button_pictureBox6_Click" Opacity="0.2"/>
        <Image x:Name="pictureBox7" Source="images/pic7.png" HorizontalAlignment="Left" Height="69" Margin="28,234,0,0" VerticalAlignment="Top" Width="52"/>
        <Button x:Name="button_pictureBox7" HorizontalAlignment="Left" Margin="23,230,0,0" VerticalAlignment="Top" Width="52" Background="{x:Null}" BorderBrush="Black" Height="69" Click="button_pictureBox7_Click" Opacity="0.2"/>
        <Image x:Name="pictureBox8" Source="images/pic8.png" HorizontalAlignment="Left" Height="69" Margin="98,240,0,0" VerticalAlignment="Top" Width="52"/>
